<h1>Add New College</h1>
<hr>
<% @college.errors.full_messages.each do |error| %>
    <ul>
      <li><%= error %></li>
    </ul>
<% end %>
<%= form_for :college, url: colleges_path do |f| %>
    <div class="field">
      <%= f.label 'College name' %>
      <%= f.text_field :name %>
    </div>

    <div class="field">
      <%= f.label :established_year %>
      <%= f.text_field :established_year %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>

<hr>.
<%= link_to 'Back', colleges_path %>
